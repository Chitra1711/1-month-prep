 public static String pangrams(String s) {
        s = s.toLowerCase();
        String str = "";
        boolean flag = true;
        for(char ch = 'a'; ch<='z'; ch++){
            if(!s.contains(String.valueOf(ch))){
                flag = false;
            }
            
        }
        if(flag)
            str +="pangram";
        else 
            str +="not pangram";
        return str;


    }

}
